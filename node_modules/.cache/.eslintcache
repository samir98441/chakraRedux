) => {\n    dispatch(loadCart);\n  }, []);\n\n  return (\n    <Flex>\n      <Box w=\"80%\">\n        {cart.map((item) => {\n          return (\n            <CartItem\n              key={item.PId}\n              PId={item.PId}\n              PName={item.PName}\n              Price={item.Price}\n              quantity={item.quantity}\n            />\n          );\n        })}\n      </Box>\n      <Box\n        style={{ boxShadow: \"5px 5px 10px #8888\" }}\n        p=\"20px\"\n        h=\"150px\"\n        mt=\"50px\"\n      >\n        <Box>\n          <Text as=\"b\" fontSize=\"3xl\">\n            Subtotal\n          </Text>\n        </Box>\n        <Box>{total}</Box>\n      </Box>\n    </Flex>\n  );\n};\n\nexport default CartPage;\n","/home/sameer/Desktop/chakraRedux/src/constants/constants.js",[],[],"/home/sameer/Desktop/chakraRedux/src/components/footer/Footer.js",[],[],"/home/sameer/Desktop/chakraRedux/src/components/header/Header.js",["296"],[],"import React from \"react\";\nimport \"./header.css\";\nimport { Link, useNavigate } from \"react-router-dom\";\nimport { Avatar, Button } from \"@chakra-ui/react\";\nimport Search from \"../search/Search\";\nimport LogoutIcon from \"@mui/icons-material/Logout\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { logout } from \"../../store/slices/loginSlice\";\nimport ShoppingCartOutlinedIcon from \"@mui/icons-material/ShoppingCartOutlined\";\n\nconst Header = () => {\n  const dispatch = useDispatch();\n  const cart = useSelector((state) => state.CartReducer.cart);\n  const navigate = useNavigate();\n  const handleHomeClick = () => {\n    navigate(\"/user\");\n  };\n  return (\n    <div className=\"header\">\n      <div className=\"logo\">\n        <Link to=\"/\">ChakraLogO</Link>\n      </div>\n      <div className=\"searchbar\">\n        <Search />\n      </div>\n      <div className=\"nav\">\n        <ul>\n          <li>\n            <Avatar\n              name=\"Sameer kayastha\"\n              src=\"